   "source": [
    "import numpy as np\n",
    "from GMM import EM_GMM\n",
    "import pandas as pd\n",
    "%matplotlib inline\n",
    "\n",
    "X = np.loadtxt('data.txt',delimiter=',')\n",
    "\n",
    "k=2\n",
    "model = EM_GMM(X,K=k,n_iter=100)\n",
    "model.fit()\n",
    "pd.Series(model.logLikelihoods).plot()"
   ]
